{
  "name": "ocr4all-frontend",
  "version": "0.0.1-alpha",
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vite build",
    "preview": "vite build && vite preview --port 3333",
    "start": "yarn dev & wait-on tcp:3333 -v",
    "test": "vitest",
    "lint": "eslint ./src",
    "format": "prettier . --write",
    "coverage": "vitest run --coverage",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test-e2e": "playwright test -c playwright.config.ts",
    "test:ci": "vitest",
    "test:ci-e2e": "playwright test",
    "test:ci-ct": "playwright test -c playwright-ct.config.ts",
    "deps:update": "taze -I major"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.13",
    "@heroicons/vue": "^2.0.18",
    "@vee-validate/i18n": "^4.9.6",
    "@vueuse/components": "^10.1.2",
    "baklavajs": "^2.0.2-beta.3",
    "pinia": "2.1.3",
    "primeicons": "^6.0.1",
    "primevue": "^3.29.2",
    "vee-validate": "^4.9.6",
    "vue": "3.3.4",
    "vue-router": "4.2.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@pinia/testing": "0.1.2",
    "@playwright/experimental-ct-vue": "1.34.3",
    "@playwright/test": "1.34.3",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/test-utils": "2.3.2",
    "@vueuse/core": "^10.1.2",
    "autoprefixer": "10.4.14",
    "c8": "7.14.0",
    "cssnano": "6.0.1",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-vue": "9.14.1",
    "happy-dom": "9.20.3",
    "postcss": "8.4.24",
    "postcss-import": "15.1.0",
    "postcss-nesting": "11.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "3.3.2",
    "taze": "^0.10.1",
    "typescript": "5.0.4",
    "unplugin-auto-import": "0.16.4",
    "unplugin-vue-components": "0.25.0",
    "vite": "4.3.9",
    "vitest": "0.31.3",
    "vue-i18n": "9.2.2",
    "wait-on": "7.0.1"
  }
